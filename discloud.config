{
  "AME": "StatsCore",            // Nombre de tu aplicación (puedes mantenerlo igual)
  "AVATAR": "https://imgur.com/a/HHYQwl7",  // Avatar (lo puedes dejar igual si lo prefieres)
  "TYPE": "bot",                 // Tipo de aplicación, sigue siendo "bot"
  "MAIN": "bot.py",              // Aquí cambia el archivo principal a "bot.py" (el archivo que ejecuta el bot)
  "RAM": 100,                    // Cantidad de RAM, sigue igual
  "AUTORESTART": true,           // Auto reiniciar, sigue igual
  "VERSION": "latest",           // Versión, sigue igual
  "APT": "tools",                // Puedes mantener esta opción, si necesitas herramientas adicionales
  "runtime": "python",           // Añadido para especificar que es una aplicación Python
  "env": "python3.11"            // Aquí defines la versión de Python que quieres usar
}
